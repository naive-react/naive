{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "paths": {
            "@/*": [
                "src/*"
            ],
            "components/*": [
                "src/components/*"
            ],
            "utils/*": [
                "src/utils/*"
            ],
        },
        "extends": [
            "node_modules/",
            "./index.html"
        ]
    }
}